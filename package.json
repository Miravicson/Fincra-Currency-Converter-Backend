  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@prisma/client",
      "@prisma/engines",
      "@scarf/scarf",
      "@snaplet/seed",
      "@swc/core",
      "argon2",
      "bcrypt",
      "esbuild",
      "msgpackr-extract",
      "nestjs-pino",
      "prisma"
