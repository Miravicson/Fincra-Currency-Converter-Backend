    "ignoredBuiltDependencies": [
      "msgpackr-extract"
    ]
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": " cross-env-shell nest start --watch",
    "start:debug": "env-cmd -f development.env cross-env-shell nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "env-cmd -f test.env cross-env-shell jest",
    "test:watch": "env-cmd -f test.env cross-env-shell jest --watch",
    "test:cov": "env-cmd -f test.env cross-env-shell jest --coverage",
    "test:debug": "env-cmd -f test.env cross-env-shell node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd -f test.env cross-env-shell jest --config ./test/jest-e2e.json",
    "test:int": "env-cmd -f test.env cross-env-shell ./scripts/run-integration.sh",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "prod:prisma:studio": "cross-env NODE_ENV=production npx prisma studio",
    "prisma:migrate:dev": "prisma migrate dev --skip-seed",
    "prisma:generate": "npx prisma generate",
    "prisma:seed:dev": "cross-env NODE_ENV=development prisma db seed",
    "prisma:seed:prod": "cross-env NODE_ENV=production prisma db seed",
    "prisma:reset:db": "pnpm prisma migrate reset --skip-seed",
    "repl": "env-cmd -f development.env cross-env-shell -- pnpm start --watch --entryFile repl",
    "crun:dev": "env-cmd -f development.env cross-env-shell ts-node -r tsconfig-paths/register src/main-cli.ts",
    "crun": "cross-env NODE_ENV=production node dist/src/main-cli.js",
    "email:dev": "email dev -p 3001 --dir src/mail/templates"
  },
  "resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
